{
  "appId": "com.electron-buddy.playground",
  "directories": {
    "output": "release/${version}"
  },
  "files": [
    "dist"
  ],
  "nsis": {
    "deleteAppDataOnUninstall": true,
  },
  "win": {
    "target": [
      {
        "target": "nsis",
        "arch": [
          "x64",
        ]
      }
    ]
  },
  "mac": {
    "target": [
      {
        "target": "dmg",
        "arch": [
          "universal"
        ]
      }
    ]
  }
}
